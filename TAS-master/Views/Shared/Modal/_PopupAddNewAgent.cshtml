
<div class="modal fade bd-example-modal-lg" id="AddNewRubberAgent" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Thêm mới thông tin nhà vườn</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <!-- [ form-element ] start -->
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="mb-3">
                                    <label class="form-label">Mã đại lý: *</label>
                                    <input type="text" class="form-control p-2" placeholder="nhập mã nhà vườn" id="agentCode">
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="mb-3 pe-2">
                                    <label class="form-label">Tên đại lý: *</label>
                                    <input type="text" class="form-control p-2" placeholder="nhập tên đại lý" id="agentName">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="mb-3 pe-2">
                                    <label class="form-label">Chủ đại lý: </label>
                                    <input type="text" class="form-control p-2" placeholder="nhập chủ đại lý" id="ownerName">
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="mb-3 pe-2">
                                    <label class="form-label">Địa chỉ</label>
                                    <input type="text" class="form-control p-2" placeholder="nhập địa chỉ" id="agentAddress">
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="mb-3 pe-2">
                                    <label class="form-label">Điện thoại:</label>
                                    <input type="text" class="form-control p-2" placeholder="nhập điện thoại" id="taxCode">
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- [ form-element ] end -->
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" data-i18n="key_close"></button>
                <button type="button" class="btn btn-success" data-bs-dismiss="modal" onclick="UpdateDataAfterEdit(1)" data-i18n="key_save">Save</button>
            </div>
        </div>
    </div>
</div>